{
    "name": "piuts-api",
    "version": "1.0.0",
    "description": "Example file service",
    "scripts": {
        "dev": "nodemon index.ts",
        "start:local": "npm run build-ts && set ENV=local && cp -r environments/. dist/environments && node dist/index.js",
        "start:dev": "npm run build-ts && set ENV=dev && cp -r environments/. dist/environments && node dist/index.js",
        "build-ts": "tsc --sourceMap false",
        "build": "npm run build-ts && cp -r environments/. dist/environments && cp package.json Dockerfile ./dist/",
        "start_serve": "npm i --production && export ENV=dev && node index.js"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.3",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "bcrypt": "^5.0.1",
        "bcrypt-nodejs": "0.0.3",
        "chalk": "^4.1.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-validator": "^6.12.0",
        "jsonwebtoken": "^8.5.1",
        "jwt-simple": "^0.5.6",
        "moment": "^2.29.1",
        "multer": "^1.4.4",
        "mysql2": "^2.2.5",
        "node-cron": "^3.0.0",
        "nodemailer": "^6.7.2",
        "request": "^2.88.2",
        "sequelize": "^6.6.5",
        "socket.io": "^4.4.1",
        "string-random": "^0.1.3",
        "uuidv4": "^6.2.12"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/bcrypt-nodejs": "0.0.31",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/express-validator": "^3.0.0",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/moment": "^2.13.0",
        "@types/multer": "^1.4.7",
        "@types/mysql": "^2.15.19",
        "@types/node-cron": "^3.0.1",
        "@types/nodemailer": "^6.4.4",
        "@types/request": "^2.48.8",
        "@types/uuid": "^8.3.4",
        "nodemon": "^2.0.12",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5"
    },
    "author": "Antpack",
    "license": "ISC"
}
